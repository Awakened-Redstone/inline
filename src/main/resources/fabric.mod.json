{
  "schemaVersion": 1,
  "id": "inline",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "samsthenerd"
  ],
  "contact": {
    "homepage": "https://github.com/SamsTheNerd/inline",
    "sources": "https://github.com/SamsTheNerd/inline",
    "issues": "https://github.com/SamsTheNerd/inline/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/inline/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.samsthenerd.inline.platform.InlineWrapper"
    ],
    "client": [
      "com.samsthenerd.inline.platform.InlineClientWrapper"
    ]
  },
  "mixins": [
    "inline.mixins.json"
  ],
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_2583": ["com/samsthenerd/inline/impl/InlineStyle"],
      "net/minecraft/component/type/ProfileComponent": ["com/samsthenerd/inline/impl/ProfileComponentUtil"]
    }
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_dependency}",
    "fabric-api": "*",
    "configurable": "*"
  },
  "recommends": {
    "yet_another_config_lib_v3": "*"
  },
  "breaks": {
    "emojiful": "*"
  }
}
